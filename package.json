{
  "name": "dsa",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.11.0 <25.0.0",
    "pnpm": ">=9.0.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "lint": "eslint . --cache --cache-location .eslintcache --ext .js,.jsx",
    "lint:fix": "eslint . --cache --cache-location .eslintcache --ext .js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md,css,html}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,json,md,css,html}\"",
    "check": "npm run format:check && npm run lint",
    "docs": "jsdoc -c jsdoc.json",
    "docs:watch": "nodemon --watch src --ext js,jsx --exec \"pnpm run docs\"",
    "docs:serve": "live-server docs",
    "docs:dev": "concurrently \"pnpm run docs:watch\" \"pnpm run docs:serve\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.25.0",
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@vitest/coverage-v8": "4.0.15",
    "@vitest/ui": "4.0.15",
    "better-docs": "2.7.3",
    "concurrently": "9.2.1",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "61.5.0",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-unused-imports": "4.3.0",
    "globals": "16.5.0",
    "jsdoc": "4.0.5",
    "live-server": "1.2.2",
    "nodemon": "3.1.11",
    "prettier": "3.7.4",
    "vitest": "4.0.15"
  }
}
